{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "PGHOST": "@pghost",
    "PGDATABASE": "@pgdatabase",
    "PGUSER": "@pguser",
    "PGPASSWORD": "@pgpassword",
    "PGSSLMODE": "require",
    "PGCHANNELBINDING": "require",
    "PGPORT": "5432",
    "JWT_SECRET": "@jwt_secret"
  }
}